<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>

    <link rel="stylesheet" href="/css/bootstrap.css">
    <link rel="stylesheet" href="/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css">
    <!-- files for statistics -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/waypoints/4.0.1/jquery.waypoints.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Counter-Up/1.0.0/jquery.counterup.min.js"></script>

    <script src="/js/bootstrap.bundle.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js"></script>
    <!--imp-->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.2/css/all.min.css" />


    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

   
  

    <style>
        /* Note: Try to remove the following lines to see the effect of CSS positioning */
        .affix {
            top: 0;
            width: 100%;
            z-index: 9999 !important;
            position: fixed;
        }

            .affix + .container-fluid {
                padding-top: 70px;
            }
    </style>


</head>
<body>


    <div class="top_header w-100 ">
        <div class="ml-auto ">

            <h6 class="top_OfficeTime mr-5"> Mon - Sat: 9:30 AM - 7:30 PM</h6> <h6 class="top_ContactNo mr-5"> Contact : +91 99 60 660 330</h6>

        </div>
    </div>
    <div class="container-fluid">

        <div class="row" id="navRow">
            <nav class="navbar  navbar-expand-lg mt-3 navbar-dark bg-primary  " data-spy="affix" data-offset-top="197">
                <div class="container-fluid ">
                    <a class="navbar-brand" href="#">
                        <img class="" src="~/img/Infosonic Logo Website.png" alt="">
                    </a>
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                            data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                            aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon">
                            <i class="fas fa-bars" style="color:#56519C; font-size:28px;"></i>
                        </span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <ul class="navbar-nav ml-auto mb-2 mb-lg-0">
                            <li class="nav-item">
                                @Html.ActionLink("Home", "Index", "Home", new { @Class = "nav-link" })
                            </li>
                            <li class="nav-item">
                                @Html.ActionLink("ABOUT", "About", "Home", new { @Class = "nav-link" })
                            </li>
                          
                            <li class="nav-item">
                                @Html.ActionLink("Services", "Services", "Home", new { @Class = "nav-link" })

                            </li>
                            @*<li class="nav-item">
                                @Html.ActionLink("Portfolio", "Portfolio", "Home", new { @Class = "nav-link" })

                            </li>*@
                            <li class="nav-item">
                                @Html.ActionLink("Careers", "Careers", "Home", new { @Class = "nav-link" })

                            </li>
                            <li class="nav-item">
                                @Html.ActionLink("Downloads", "Downloads", "Home", new { @Class = "nav-link" })

                            </li>
                            <li class="nav-item">
                                @Html.ActionLink("Contact Us", "Contact", "Home", new { @Class = "nav-link" })

                            </li>


                        </ul>
                        <!-- <form class="d-flex" role="search">
                          <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                          <button class="btn btn-outline-success" type="submit">Search</button>
                        </form> -->
                    </div>
                </div>
            </nav>
        </div>

    </div>





    @RenderBody()







    <div class="container-fluid">


        <section id="footerSection">
            <div class="container-fluid ">
                <div class="row justify-content-center">
                    <div class="col-md-3 footerColumn m-2 ">

                        <img src="~/img/Infosonic Logo Website.png" class="text-center img-fluid mt-3" alt="No Image">
                        <p class="mt-3">
                            We firmly believe in Quality Delivery with Best Return of Investment (ROI) yet Cost
                            Effective Solutions. Get to know us better…

                            <div class="text-center mb-2 mt-3">
                                <button class="text-uppercase btn">Contact Us </button>
                            </div>
                        </p>
                    </div>
                    <div class="col-md-3 footerColumn m-2">
                        <div class="footerTitle">
                            <h5 class="mt-3 mb-1 ml-3">Our Services </h5>
                        </div>
                        <ul class="mt-4">
                            <li>
                                <a href="">Web Application</a>
                            </li>
                            <li>
                                <a href="">Android Application</a>
                            </li>
                            <li>
                                <a href="">Banking Application</a>
                            </li>
                            <li>
                                <a href="">Digital Solutions</a>
                            </li>
                            <li>
                                <a href="">Desktop Applications</a>
                            </li>
                        </ul>

                    </div>
                    <div class="col-md-3 footerColumn m-2">
                        <div class="footerTitle">
                            <h5 class="mt-3 mb-1 ml-3">Contact Info </h5>
                        </div>
                        <div class="footerTitle">
                            <h6 class="mt-3  ml-3">Address </h6>
                        </div>
                        <p class="ml-3">Lorem ipsum dolor sit amet consectetur adipisicing elit. Deleniti, iste!</p>
                        <div class="footerTitle">
                            <h6 class="mt-1 ml-3">Business Hours</h6>
                        </div>
                        <p class="ml-3">Mon - Sat: 9:30 AM - 7:30 PM</p>
                        <div class="footerTitle">
                            <h6 class="mt-1 ml-3">Phone Number </h6>
                        </div>
                        <p class="ml-3"> +91 99 60 660 330 </p>
                        <h6 class="mt-3  ml-3">Social Links Here..... </h6>

                    </div>

                </div>
            </div>
        </section>
        <section id="footer">
            <div class="row justify-content-center">
                <div class="col-lg-5">
                    <p class="mt-1">
                        © Infosonic Software Solutions Pvt Ltd - 2013. All rights reserved.
                    </p>
                </div>

            </div>
        </section>

    </div>
    <script src="https://code.jquery.com/jquery-3.6.1.js"
            integrity="sha256-3zlB5s2uwoUzrXK3BT7AX3FyvojsraNFxCc2vC/7pNI=" crossorigin="anonymous"></script>


    <script>

        $(document).ready(function () {
            $('.list').click(function () {
                var value = $(this).attr('data-filter');
                if (value == 'All') {
                    $('.itemBox').show('1000');

                }
                else {
                    $('.itemBox').not('.' + value).hide('1000');
                    $('.itemBox').filter('.' + value).show('1000');

                }
            })
        });
    </script>
    <script src="https://code.jquery.com/jquery-3.6.1.js"
            integrity="sha256-3zlB5s2uwoUzrXK3BT7AX3FyvojsraNFxCc2vC/7pNI=" crossorigin="anonymous"></script>
    <script src="bootstrap.bundle.js"></script>
    <script src="https://code.jquery.com/jquery-2.1.3.min.js"></script>
    <!-- <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js"></script> -->
    <script src="/css/js/bootstrap.bundle.min.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
